{
  "name": "signal-stack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "pre-commit run --all-files",
    "lint:staged": "pre-commit run",
    "test": "pre-commit run --all-files",
    "test:ci": "pre-commit run --all-files",
    "typecheck": "pre-commit run typescript-check --all-files",
    "lint:md": "pre-commit run markdownlint --all-files",
    "test:links": "pre-commit run markdown-link-check --all-files",
    "test:build": "pre-commit run build-check --all-files",
    "format": "prettier --write \"docs/**/*.md\" \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"docs/**/*.md\" \"src/**/*.{js,jsx,ts,tsx}\"",
    "validate:deploy": "npm run build && npm run serve",
    "github:test": "act --platform ubuntu-latest=catthehacker/ubuntu:act-latest",
    "github:test:deploy": "act push --platform ubuntu-latest=catthehacker/ubuntu:act-latest --workflows .github/workflows/deploy.yml",
    "github:test:pr": "act pull_request --platform ubuntu-latest=catthehacker/ubuntu:act-latest --workflows .github/workflows/test.yml"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "markdown-link-check": "^3.13.7",
    "markdownlint-cli": "^0.45.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
